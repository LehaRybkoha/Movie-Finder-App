;AAMA;AAjCA,CAAA","sourcesContent":["<template>\n    <section class=\"movie\">\n        <ul class=\"movie__list\">\n            <v-movie-item\n                    v-for=\"movie in filteredList\"\n                    :key=\"movie.id\"\n                    :movie_data=\"movie\"\n            />\n        </ul>\n    </section>\n</template>\n\n<script>\n    import vMovieItem from './v-movie-item'\n    import { mapActions, mapGetters } from 'vuex'\n\n    export default {\n        name: 'v-movie-list',\n        components: {\n            vMovieItem\n        },\n        computed: {\n            movieFilteredList() {\n               return this.$store.getters.MOVIE_INPUT_DATA\n            },\n            ...mapGetters([\n                'MOVIES',\n                'MOVIE_INPUT_DATA'\n            ]),\n            filteredList: function() {\n                let movie = this.movieFilteredList;\n                return this.MOVIES.filter(function (elem) {\n                    if (movie === '') return true;\n                    else return elem.title.toLowerCase().indexOf(movie.toLowerCase()) > -1;\n                })\n            }\n        },\n        methods: {\n            ...mapActions([\n                'GET_MOVIES_FROM_API'\n            ]),\n        },\n        mounted() {\n            this.GET_MOVIES_FROM_API()\n                .then((response) => {\n                    if (response.data) {\n                        console.log('Data arrived')\n                    }\n                })\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n    .movie {\n        max-width: 1200px;\n        margin: 0 auto;\n        &__list {\n            display: flex;\n            position: relative;\n            flex-wrap: wrap;\n            justify-content: space-around;\n        }\n    }\n</style>"],"sourceRoot":"src/components"}]}