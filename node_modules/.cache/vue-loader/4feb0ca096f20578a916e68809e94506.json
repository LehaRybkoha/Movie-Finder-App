  data() {\n            return {\n                closing: false\n            }\n        },\n        props: {\n            movie_data: {\n                type: Object,\n                default() {\n                    return {}\n                }\n            }\n        },\n        methods: {\n            open() {\n                this.closing = true;\n                this.$refs[\"movie__item\"].classList.add('open-item');\n                this.$refs[\"movie__item-img\"].classList.add('open-img');\n                document.querySelector('.v-modal-overlay').classList.add('open-overlay');\n            },\n            close() {\n                this.closing = false;\n                this.$refs[\"movie__item\"].classList.remove('open-item');\n                this.$refs[\"movie__item-img\"].classList.remove('open-img');\n                document.querySelector('.v-modal-overlay').classList.remove('open-overlay');\n            },\n            toggleItem() {\n                if (this.closing) {\n                    this.close()\n                } else {\n                    this.open()\n                }\n            }\n        }\n    }\n</script>\n\n\n<style lang=\"scss\">\n    .movie  {\n        &__item {\n            margin: 10px;\n            cursor: pointer;\n            &-title {\n                text-align: center;\n            }\n            &-year {\n                text-align: center;\n            }\n            &-img {\n                width: 220px;\n            }\n        }\n    }\n    .open-item {\n        position: fixed;\n        background-color: rgba(255, 255, 255, 0.8);\n        z-index: 10;\n    }\n    .open-img {\n        width: 400px;\n    }\n</style>"]}]}